{
  "name": "vue-cycle-timer",
  "version": "1.0.2",
  "author": "BaiHuaiyu",
  "description": "vue3循环定时器",
  "repository": "https://github.com/ResJI/vue-cycle-timer.git",
  "homepage": "https://github.com/ResJI/vue-cycle-timer.git",
  "keywords": [
    "timer",
    "vue"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite -c ./vite.config.app.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-demo": "run-p type-check \"build-only -- -c ./vite.config.app.ts\"",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "vue": "^3.3.11",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^1.8.25"
  }
}
